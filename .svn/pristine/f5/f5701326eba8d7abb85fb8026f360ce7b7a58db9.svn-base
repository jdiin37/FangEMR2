}
.btn-primary-inverse.disabled:hover,
.btn-primary-inverse[disabled]:hover,
fieldset[disabled] .btn-primary-inverse:hover,
.btn-primary-inverse.disabled:focus,
.btn-primary-inverse[disabled]:focus,
fieldset[disabled] .btn-primary-inverse:focus,
.btn-primary-inverse.disabled.focus,
.btn-primary-inverse[disabled].focus,
