/**EMR checkBox***/

.EMRCheckBox {
    display:none;
}
input[type="checkbox"] + label span {
    display:inline-block;
    width:20px;
    height:20px;
    margin:-2px 5px 0 0;
    vertical-align:middle;
    /**background:url(/FangEmrServices/img/ic_check_off.png) left top no-repeat;**/
    background:url(/FangEmrServices/img/unchecked.png)no-repeat;
    cursor:pointer;
}
input[type="checkbox"]:checked + label span {
    background:url(/FangEmrServices/img/checked.png)no-repeat;
    /**background:url(/FangEmrServices/img/ic_check_on.png) -30px top no-repeat;**/
}

.EMRRadioButton{
  display:none;	
}

input[type="radio"] + label span {
    display:inline-block;
    width:20px;
    height:20px;
    margin:-2px 5px 0 0;
    vertical-align:middle;
    /**background:url(/FangEmrServices/img/ic_check_off.png) left top no-repeat;**/
    background:url(/FangEmrServices/img/RadioButton_unchecked.png)no-repeat;
    cursor:pointer;
}
input[type="radio"]:checked + label span {
    background:url(/FangEmrServices/img/RadioButton_checked.png)no-repeat;
    /**background:url(/FangEmrServices/img/ic_check_on.png) -30px top no-repeat;**/
}




