body {
	background-color: #ffffff;
	font-family: 微軟正黑體, arial, helvetica, sans-serif;
}

.no-close .ui-dialog-titlebar-close {display: none }

.popUp {   
    background-color: #fff;
  -webkit-background-clip: padding-box;
          background-clip: padding-box;
  border: 1px solid #999;
  border: 1px solid rgba(0, 0, 0, .2);
  /*border-radius: 1px;*/
  z-index: 1050;
  -webkit-box-shadow: 0 3px 9px rgba(0, 0, 0, .5);
          box-shadow: 0 3px 9px rgba(0, 0, 0, .5);
}

.dialog-a{
	background: -webkit-linear-gradient(180deg,#A38EC9,#D7C4FB);
	background: -o-linear-gradient(180deg,#A38EC9,#D7C4FB);
	background: -moz-linear-gradient(180deg,#A38EC9,#D7C4FB);
	background: linear-gradient(180deg,#A38EC9,#D7C4FB);
	color: #402069;
	font-weight:bold;
}

.ui-dialog-titlebar {
      background:linear-gradient(180deg,#A38EC9,#D7C4FB);
}


a:hover, a:active,a:focus ,a
{
   text-decoration: none;
}

.pre-scrollable{
	max-height: 500px;	
}
.panel{
	margin-bottom:0px;
}

.modal {
	text-align: center;
	padding: 0 !important;
}

.modal:before {
	content: '';
	display: inline-block;
	height: 100%;
	vertical-align: middle;
	margin-right: -4px;
}

.modal-dialog {
	display: inline-block;
	text-align: left;
	vertical-align: middle;
}

.well-sm {
	margin-left: 0px;
	margin-right: 0px;
}

.clickable-row {
	cursor: pointer;
}

.navbar-custom {
	/*background-color:#229922;*/
	background-color: #337ab7;
	border-color: #2e6da4;
	color: #ffffff;
	border-radius: 0;
}

.navbar-custom .navbar-nav>li>a {
	color: #fff;
}

.navbar-custom .navbar-nav>.active>a {
	color: #ffffff;
	background-color: transparent;
}

.navbar-custom .navbar-nav>li>a:hover, .navbar-custom .navbar-nav>li>a:focus,
	.navbar-custom .navbar-nav>.active>a:hover, .navbar-custom .navbar-nav>.active>a:focus,
	.navbar-custom .navbar-nav>.open>a {
	text-decoration: none;
	/*background-color: #33aa33;*/
	background-color: #286090;
	border-color: #204d74;
}

.navbar-custom .navbar-brand {
	color: #eeeeee;
}

.navbar-custom .navbar-toggle {
	background-color: #eeeeee;
}

.navbar-custom .icon-bar {
	/*background-color:#33aa33;*/
	background-color: #286090;
}

.navbar-custom {
	background-repeat: repeat-x;
	/*border-radius: 4px;*/
	-webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, .15), 0 1px 5px
		rgba(51, 122, 183, .075);
	box-shadow: inset 0 1px 0 rgba(255, 255, 255, .15), 0 1px 5px
		rgba(51, 122, 183, .075);
	background-image: linear-gradient(to bottom, #337ab7 0, #265a88 100%);
}